<h1 class="title";>Ryokan</h1>

<button><%= link_to "Home", index_index_path, :class => "header" %></button>

<h1>dogshit</h1>
<p>List of all property entries</p>

<% @Properties.each do |property|%>
    <div class="card">
        <div class="container">
          <h4><b>Location</b></h4>
          <p><%= property.address %></p>
          <p>Price: <%= property.price %></p>
          <p>Size: <%= property.size %></p>
          <p>Description: <%= property.description %></p>
          <p>Date Listed: <%= property.date_listed %></p>
          <p>Source: <%= property.url %></p>
          <button>More Info</button>
        </div>
    </div>
    
    <% end %>